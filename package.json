{
  "name": "authentication",
  "version": "1.8.4",
  "description": "Authentication - Microservice",
  "main": "Build/App.js",
  "types": "Build/App.d.ts",
  "scripts": {
    "dev::migrate": "cross-env NODE_ENV=development node --env-file=./.env Build/App.js migrate",
    "dev::rollback": "cross-env NODE_ENV=development node --env-file=./.env Build/App.js migrate -ra",
    "dev::seed": "cross-env NODE_ENV=development node --env-file=./.env Build/App.js seed",
    "dev::build": "ts-node esbuild.config.ts",
    "dev::start": "cross-env NODE_ENV=development node --env-file=./.env Build/App.js",
    "dev": "npm run dev::build && concurrently -c \"bgYellow.bold,bgBlue.bold\" \"npm:dev::watch*\"",
    "dev::watch::build": "ts-node esbuild.config.ts watch",
    "dev::watch::start": "cross-env NODE_ENV=development node --watch --env-file=./.env Build/App.js",
    "fix-lint": "eslint --fix . --ext .ts",
    "lint": "eslint . --ext .ts",
    "prod::migrate": "node Build/App.js migrate",
    "prod::rollback": "node Build/App.js migrate -ra",
    "prod::seed": "node Build/App.js seed",
    "prod::build": "tsc --noEmit && ts-node esbuild.config.ts prod-build",
    "prod::start": "node Build/App.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andesite-Lab/Authentication.git"
  },
  "keywords": [
    "andesite",
    "andesite-authentification",
    "authentification"
  ],
  "author": "Ruby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Andesite-Lab/Authentication/issues"
  },
  "homepage": "https://github.com/Andesite-Lab/Authentication#readme",
  "devDependencies": {
    "@tsconfig/node21": "^21.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.2",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@basalt-lab/basalt-auth": "^1.1.6",
    "@basalt-lab/basalt-helper": "^1.2.2",
    "@basalt-lab/basalt-logger": "^1.6.1",
    "@basalt-lab/basalt-socket": "^1.4.1",
    "@faker-js/faker": "^8.4.1",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@types/source-map-support": "^0.5.10",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "fastify": "^4.26.2",
    "i18next": "^23.10.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "source-map-support": "^0.5.21"
  }
}
